{
    "name": "isakzhanov-r/laravel-menus",
    "description": "Package for using menu construct in project laravel",
    "keywords": ["menu", "laravel menu"],
    "type": "library",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/isakzhanov-r/laravel-menus/issues",
        "source": "https://github.com/isakzhanov-r/laravel-menus"
    },
    "authors": [
        {
            "name": "isakzhanovr",
            "email": "isakzhanov.r@gmail.com",
            "homepage": "https://github.com/isakzhanov-r/laravel-menus"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "doctrine/dbal": "^2.6",
        "illuminate/support": "^7.0|^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^0.9|^1.0",
        "orchestra/testbench": "^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "IsakzhanovR\\Menus\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "IsakzhanovR\\Menus\\ServiceProvider"
            ]
        }
    }
}

{
    "name": "isakzhanov-r/laravel-menus",
    "description": "Package for using user roles and permissions in project laravel",
    "keywords": ["laravel", "permissions", "user roles", "model permission", "user permissions", "roles permissions"],
    "type": "library",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/isakzhanov-r/laravel-menus/issues",
        "source": "https://github.com/isakzhanov-r/laravel-menus"
    },
    "authors": [
        {
            "name": "isakzhanovr",
            "email": "isakzhanov.r@gmail.com",
            "homepage": "https://github.com/isakzhanov-r/laravel-menus"
        }
    ],
    "require": {
        "php": "^7.1.3|^8.0",
        "doctrine/dbal": "^2.6",
        "illuminate/support": "^5.5|^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "IsakzhanovR\\Menus\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "IsakzhanovR\\Menus\\ServiceProvider"
            ]
        }
    }
}
